{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "quotes": [2, "single", "avoid-escape"],
       "curly": 2,
       "import/prefer-default-export":"off",
       "linebreak-style":"off",
       "react/prop-types": "off",
       "jsx-a11y/label-has-associated-control": "off",
       "jsx-a11y/label-has-for":"off"
    }
}
